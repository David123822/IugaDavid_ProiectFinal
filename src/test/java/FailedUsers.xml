        <parameter name="username" value="Alice"/>
        <parameter name="password" value="alicepwd!"/>
        <classes>
            <class name="FailedLogin"/>
        </classes>
    </test>

    <test name="Fail Login2">
        <parameter name="username" value="Mihail"/>
        <parameter name="password" value="mihailpwd22"/>
        <classes>
            <class name="FailedLogin"/>
        </classes>
    </test>

    <test name="Fail Login3">
        <parameter name="username" value="Bob"/>
        <parameter name="password" value="bobspassword!"/>
        <classes>
            <class name="FailedLogin"/>
        </classes>
    </test>

    <test name="Fail Login4">
        <parameter name="username" value="David"/>
        <parameter name="password" value="qwerty21!"/>
        <classes>
            <class name="FailedLogin"/>
        </classes>
    </test>
</suite>
